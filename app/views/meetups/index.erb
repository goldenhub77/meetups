<div class="row">
  <div class="small-12 medium-12 large-12 columns">
    <h1>Meetups</h1>
  </div>
</div>
<div class="row">
  <% all_sorted_meetups.each do |meetup| %>
    <div class="small-12 medium-6 large-4 columns">
      <a class="meetup-link" href="/meetups/<%= meetup.id %>">
        <h5><%= meetup.name %></h5>
        <p>Members: <%= meetup.users.length %></p>
      </a>
    </div>
  <% end %>
</div>
